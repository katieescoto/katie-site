{
    "name": "thinkshout/drupal-project",
    "description": "ThinkShout's project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "ThinkShout",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "git",
            "url": "https://github.com/thinkshout/DrupalDriver.git"
        },
        {
            "type": "git",
            "url": "https://github.com/thinkshout/drupalextension.git"
        },
        {
            "type": "git",
            "url": "https://github.com/thinkshout/behat-drush-endpoint.git"
        },
        {
            "type": "git",
            "url": "https://github.com/thinkshout/media.git"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/console": "~1.0",
        "drupal/core": "8.3.*",
        "drush/drush": "~8.0",
        "drupal/admin_toolbar": "1.19.0",
        "drupal/config_installer": "^1.4",
        "drupal/contact_storage": "1.0.0-beta8",
        "drupal/ctools": "3.0.0",
        "drupal/ds": "3.1.0",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/email_registration": "^1.0-rc5",
        "drupal/entity_reference_revisions": "1.3.0",
        "drupal/fast_404": "^1.0-alpha2",
        "drupal/field_group": "1.0.0-rc6",
        "drupal/google_analytics": "~2.1",
        "drupal/honeypot": "~1.25",
        "drupal/inline_entity_form": "~1.0-beta1",
        "drupal/linkit": "5.0.0-beta5",
        "drupal/menu_block": "~1.4",
        "drupal/metatag": "1.1",
        "drupal/migrate_google_sheets": "1.0-beta1",
        "drupal/migrate_plus": "4.0-beta1",
        "drupal/migrate_tools": "^4.0-beta1",
        "drupal/pathauto": "1.0",
        "drupal/redirect": "^1.0-alpha5",
        "drupal/redis": "^1.0-beta1",
        "drupal/simple_sitemap": "2.9",
        "drupal/token": "1.0",
        "thinkshout/media": "dev-8.x-1.x",
        "webflo/drupal-finder": "^0.3.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "drupal/devel": "^1.0-rc1",
        "drupal/drupal-driver": "dev-master",
        "drupal/drupal-extension": "dev-master",
        "drush-ops/behat-drush-endpoint": "dev-master",
        "jcalderonzumba/gastonjs": "^1.1@dev",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "pantheon-systems/ci-scripts": "*",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~2.8",
        "thinkshout/robo-drupal": "dev-master"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/core": {
                "Telephone field with 5 digits or less fatally breaks formatter https://www.drupal.org/node/2484693": "https://www.drupal.org/files/issues/2484693-47.patch",
                "Repetitive warning message about uasort": "https://www.drupal.org/files/issues/2567035-26-usort.patch",
                "Plugin Lazy loading can cause usort warning": "https://www.drupal.org/files/issues/2699157-23.drupal.Plugin-Lazy-loading-can-cause-usort-warning.patch",
                "Make menu block follow active item": "https://www.drupal.org/files/issues/menu_subtrees_in_menu-2631468-122.patch",
                "ImageWidget::validateRequiredFields() produces a PHP Warning message if triggering element is a non-button": "https://www.drupal.org/files/issues/2745491-15.patch",
                "DrupalBehaviorError: attach ; quickedit: entityElement.get(...) is undefined https://www.drupal.org/node/2551373": "https://www.drupal.org/files/issues/2551373-35.patch"
            },
            "drupal/ctools": {
                "Views exposed filters missing autosubmit option https://www.drupal.org/node/2475595": "https://www.drupal.org/files/issues/2475595-ctools-autocomplete-fix-47.patch"
            }
        },
        "drupal-scaffold": {
            "excludes": [
                ".htaccess"
            ]
        }
    }
}
